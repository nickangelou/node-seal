{
  "tags": {
    "allowUnknownTags": ["par", "warning", "optional"]
  },
  "source": {
    "include": ["./src/implementation"],
    "includePattern": "\\.(ts)$"
  },
  "opts": {
    "destination": "./docs/",
    "readme": "README.md",
    "template": "node_modules/better-docs"
  },
  "plugins": ["node_modules/better-docs/typescript"]
}
