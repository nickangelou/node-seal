{
  "name": "node-seal",
  "version": "1.0.13",
  "description": "Port of Microsoft SEAL to Javascript",
  "main": "dist/seal.node.js",
  "library": "src/main.js",
  "module": "src/main.js",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --open",
    "test": "node --max-old-space-size=16384 `which jest`",
    "test:encrypt": "node --max-old-space-size=4096 `which jest` --testPathIgnorePatterns=\"^((?!encrypt-).)*$\"",
    "test:low": "node --max-old-space-size=4096 `which jest` --testPathIgnorePatterns=\".*high.*\" \".*medium.*\"",
    "test:medium": "node --max-old-space-size=8192 `which jest` --testPathIgnorePatterns=\".*high.*\" \".*low.*\"",
    "test:high": "node --max-old-space-size=16384 `which jest` --testPathIgnorePatterns=\".*low.*\" \".*medium.*\"",
    "coverage": "node --max-old-space-size=16384 `which jest` --collectCoverageFrom=src/lib/**.js --coverage src/test",
    "build": "webpack",
    "package": "yarn build && yarn test && npm publish"
  },
  "devDependencies": {
    "esm": "^3.2.25",
    "file-loader": "^3.0.1",
    "jest": "^24.8.0",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1",
    "webpack-node-externals": "^1.7.2"
  },
  "browserslist": [
    "since 2017-06"
  ]
}
